{
  "name": "llm-image-analyzer",
  "version": "1.0.0",
  "description": "A Node.js script that uses LM Studio to analyze images, files, and text with local LLM models",
  "main": "server.js",
  "scripts": {
    "server": "node --expose-gc server.js",
    "server:openai": "node --expose-gc server.js openai/gpt-oss-20b",
    "server:gemma": "node --expose-gc server.js google/gemma-3-12b",
    "client": "node client.js",
    "run": "node client.js",
    "serve": "node --expose-gc server.js",
    "request": "node client.js",
    "openai": "node client.js -m openai/gpt-oss-20b",
    "gemma": "node client.js -m google/gemma-3-12b",
    "unload": "node client.js -u",
    "load:openai": "node client.js -l -m openai/gpt-oss-20b",
    "load:gemma": "node client.js -l -m google/gemma-3-12b",
    "help": "node client.js --help",
    "issue:json": "node client.js -m google/gemma-3-12b -i images/image.png -J -O outputs/issue.json",
    "sandbox:google": "node sandbox/google_proxy_server.js"
  },
  "type": "module",
  "dependencies": {
    "@lmstudio/sdk": "^1.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "llm",
    "image-analysis",
    "lm-studio",
    "ai",
    "vision",
    "text-analysis"
  ],
  "author": "",
  "license": "MIT"
}
